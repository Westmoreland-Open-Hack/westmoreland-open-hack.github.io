<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "Westmoreland Open Hack" %></title>
    
    <%= stylesheet_link_tag "normalize", "style" %>
    <%= javascript_include_tag  "all" %>
  </head>
  
  <body class="<%= page_classes %> site">
    <header>
      <%= partial 'header' %>
    </header>
    <%= yield %>
    <footer>
      <div class="leftCol">
      </div>
      <div class="rightCol">
      </div>
    </footer>
  </body>
</html>